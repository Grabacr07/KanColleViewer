<UserControl x:Class="Grabacr07.KanColleViewer.Plugins.ExpeditionProgressSettings"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:local="clr-namespace:Grabacr07.KanColleViewer.Plugins"
			 mc:Ignorable="d"
			 d:DesignWidth="300"
			 d:DesignHeight="300"
			 d:DataContext="{d:DesignInstance local:ExpeditionProgress}">
	<StackPanel Margin="12">
		<CheckBox Content="全艦隊が遠征に出ていないときにエラー扱いにする"
				  IsChecked="{Binding ErrorIfAllWaiting}" />
	</StackPanel>
</UserControl>
